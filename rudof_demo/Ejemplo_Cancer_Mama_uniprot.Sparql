PREFIX up: <http://purl.uniprot.org/core/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT DISTINCT ?protein ?proteinName ?disease ?diseaseName ?diseaseID
WHERE {
  # Proteínas humanas (opcional pero recomendado para reducir resultados)
  ?protein a up:Protein ;
           up:organism <http://purl.uniprot.org/taxonomy/9606> ;
           up:annotation ?annotation .
  
  # Anotaciones de tipo enfermedad
  ?annotation a up:Disease_Annotation ;
              up:disease ?disease .
  
  # Nombre de la enfermedad
  ?disease skos:prefLabel ?diseaseName .
  
  # Filtrar solo para cáncer de mama
  FILTER(CONTAINS(LCASE(?diseaseName), "breast cancer"))
  
  # Nombre de la proteína
  OPTIONAL { ?protein up:recommendedName ?recName .
             ?recName up:fullName ?proteinName . }
  
  # ID de la enfermedad (referencia externa como OMIM, MIM, etc.)
  OPTIONAL { ?disease rdfs:seeAlso ?diseaseID }
}
LIMIT 100